<script>
$(document).on('click', '.has-sub > a', function (e) {
  e.preventDefault();
  const $parent = $(this).parent();

  const isRouteActive = $parent.find('li.active').length > 0;

  // Only toggle 'active' class if no subitem is already active
  if (!isRouteActive) {
    $parent.toggleClass('active');
  }

  // Always toggle the submenu display (show/hide)
  $parent.find('.submenu').first().slideToggle(200);
});

// Show submenu by default if active
$(document).ready(function () {
  $('.has-sub.active .submenu').show();
});

</script>


  <script>
 function toggleSidebar() {
  const sidebar = document.getElementById("sidebar");
